<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="th.go.mof.fpo.model">
    <class
            name="SfiBranch"
            table="sfi_branch"
            >
        <meta attribute="sync-DAO">false</meta>
        <id
                name="id"
                type="integer"
                column="id"
                >
            <generator class="identity"/>
        </id>

        <!-- please tell Joe Hudson that the type 'nchar' could not be resolved.. defaulting to java.lang.String -->
        <property
                name="sfiCode"
                column="sfi_code"
                type="java.lang.String"
                not-null="true"
                length="10"
                />
        <property
                name="branchCode"
                column="branch_code"
                type="string"
                not-null="true"
                length="4"
                />
        <property
                name="nameEn"
                column="name_en"
                type="string"
                not-null="false"
                length="512"
                />
        <property
                name="nameTh"
                column="name_th"
                type="string"
                not-null="false"
                length="512"
                />
        <property
                name="openDate"
                column="open_date"
                type="timestamp"
                not-null="false"
                length="23"
                />
        <property
                name="createDate"
                column="create_date"
                type="timestamp"
                not-null="false"
                length="23"
                />
        <property
                name="createBy"
                column="create_by"
                type="string"
                not-null="false"
                length="64"
                />
        <property
                name="fileName"
                column="file_name"
                type="string"
                not-null="false"
                length="256"
                />
        <property
                name="originalFileName"
                column="original_file_name"
                type="string"
                not-null="false"
                length="256"
                />

        <property
                name="closeDate"
                column="close_date"
                type="timestamp"
                not-null="false"
                length="32"
                />
        <property
                name="areaCode"
                column="area_code"
                type="string"
                not-null="false"
                length="4"
                />
        <property
                name="areaName"
                column="area_name"
                type="string"
                not-null="false"
                length="256"
                />
        <property
                name="provinceCode"
                column="province_code"
                type="string"
                not-null="false"
                length="2"
                />
        <property
                name="provinceName"
                column="province_name"
                type="string"
                not-null="false"
                length="256"
                />
    </class>
</hibernate-mapping>