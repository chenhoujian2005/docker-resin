<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="th.go.mof.fpo.model">
	<class
		name="DocumentStatusLog"
		table="document_status_log"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="frequency"
			column="frequency"
			type="integer"
			not-null="false"
			length="10"
		/>
        <property
			name="datasetDate"
			column="dataset_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="dueDate"
			column="due_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="documentType"
			column="document_type"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="documentStatus"
			column="document_status"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="sendingStatus"
			column="sending_status"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="uploadDate"
			column="upload_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
        <property
			name="uploadDateOnly"
			column="upload_date_only"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="uploadBy"
			column="upload_by"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="adjust"
			column="adjust"
			type="boolean"
			not-null="false"
			length="1"
		/>
		<property
			name="adjustApprove"
			column="adjust_approve"
			type="boolean"
			not-null="false"
			length="1"
		/>
		<property
			name="adjustApproveDate"
			column="adjust_approve_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="adjustApproveBy"
			column="adjust_approve_by"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="adjustRejectDate"
			column="adjust_reject_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="adjustRejectBy"
			column="adjust_reject_by"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="adjustRejectReason"
			column="adjust_reject_reason"
			type="string"
			not-null="false"
			length="512"
		/>
        <property
			name="createDate"
			column="create_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="enable"
			column="enable"
			type="boolean"
			not-null="false"
			length="1"
		/>
		<many-to-one
			name="sfi"
			column="sfi_id"
			class="Sfi"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="dsTitle"
			column="ds_title_id"
			class="DsTitle"
			not-null="false"
		>
		</many-to-one>

	</class>	
</hibernate-mapping>