<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="th.go.mof.fpo.model">
    <class
            name="QuestionAnswer"
            table="question_answer"
            >
        <meta attribute="sync-DAO">false</meta>
        <id
                name="id"
                type="integer"
                column="id"
                >
            <generator class="identity"/>
        </id>
        <property
                name="questionDescription"
                column="question_description"
                type="java.lang.String"
                not-null="false"
                length="512"
                />
        <property
                name="answerDescription"
                column="answer_description"
                type="java.lang.String"
                not-null="false"
                length="512"
                />
        <property
                name="questionFileName"
                column="question_file_name"
                type="java.lang.String"
                not-null="false"
                length="128"
                />
        <property
                name="answerFileName"
                column="answe_file_name"
                type="java.lang.String"
                not-null="false"
                length="128"
                />
        <property
                name="questionOriginalFileName"
                column="question_original_file_name"
                type="java.lang.String"
                not-null="false"
                length="256"
                />
        <property
                name="answerOriginalFileName"
                column="answe_original_file_name"
                type="java.lang.String"
                not-null="false"
                length="256"
                />
        <property
                name="questionFileSize"
                column="question_file_size"
                type="java.lang.String"
                not-null="false"
                length="32"
                />
        <property
                name="answerFileSize"
                column="answe_file_size"
                type="java.lang.String"
                not-null="false"
                length="32"
                />
        <property
                name="createBy"
                column="create_by"
                type="string"
                not-null="false"
                length="64"
                />
        <property
                name="createDate"
                column="create_date"
                type="java.util.Date"
                not-null="false"
                />
        <property
                name="updateBy"
                column="update_by"
                type="string"
                not-null="false"
                length="64"
                />
        <property
                name="updateDate"
                column="update_date"
                type="java.util.Date"
                not-null="false"
                />
         <property
                name="status"
                column="status"
                type="boolean"
                not-null="false"
                />
        <property
                name="questionTitle"
                column="question_title"
                type="java.lang.String"
                not-null="false"
                length="512"
                />
        <many-to-one
                name="questionAnswerGroup"
                column="question_answer_group_id"
                class="QuestionAnswerGroup"
                not-null="false"
                >
        </many-to-one>
    </class>
</hibernate-mapping>