<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="th.go.mof.fpo.model">
	<class
		name="ReportTypeLog"
		table="report_type_log"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="originalViewFileName"
			column="original_view_file_name"
			type="string"
			not-null="false"
			length="1024"
		/>
        <property
			name="createDate"
			column="create_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="createBy"
			column="create_by"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="updateDate"
			column="update_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="updateBy"
			column="update_by"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="enable"
			column="enable"
			type="boolean"
			not-null="false"
			length="1"
		/>
          <!-- please tell Joe Hudson that the type 'xml' could not be resolved.. defaulting to java.lang.String -->
        <property
                name="content"
                column="content"
                type="java.lang.String"
                not-null="false"
                />
         <property
                name="contentExcel"
                column="content_excel"
                type="java.lang.String"
                not-null="false"
                />
       <property
			name="viewFileName"
			column="view_file_name"
			type="string"
			not-null="false"
			length="1024"
		/>
        <property
			name="originalExcelFileName"
			column="original_excel_file_name"
			type="string"
			not-null="false"
			length="1024"
		/>
        <property
			name="excelFileName"
			column="excel_file_name"
			type="string"
			not-null="false"
			length="1024"
		/>
		<many-to-one
			name="datasetVersion"
			column="dataset_version"
			class="DsVersion"
			not-null="false"
		>
		</many-to-one>
        <many-to-one
			name="activityLog"
			column="activity_log_id"
			class="ActivityLog"
			not-null="false"
		>
		</many-to-one>
          <many-to-one
			name="reportType"
			column="report_type_id"
			class="ReportType"
			not-null="false"
		>
		</many-to-one>

	</class>	
</hibernate-mapping>