<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="th.go.mof.fpo.model">
	<class
		name="News"
		table="news"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="actionDate"
			column="action_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
        <property
			name="endDate"
			column="end_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="description"
			column="description"
			type="string"
			not-null="false"
		/>
		<property
			name="createDate"
			column="create_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
         <property
            name="fileName"
            column="file_name"
            type="string"
            not-null="false"
            length="256"
            />
		<property
			name="createBy"
			column="create_by"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="updateDate"
			column="update_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="updateBy"
			column="update_by"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="enable"
			column="enable"
			type="boolean"
			not-null="false"
			length="1"
		/>
        <property
			name="subject"
			column="subject"
			type="string"
			not-null="false"
			length="256"
		/>
        <property
			name="sfi"
			column="sfi_id"
			type="string"
			not-null="false"
			length="128"
		/>
        <property
            name="originalFileName"
            column="original_file_name"
            type="string"
            not-null="false"
            length="256"
            />
        <property
			name="sendType"
			column="send_type"
			type="string"
			not-null="false"
			length="1"
		/>

       <property
                name="sendFlag"
                column="send_flag"
                type="java.lang.Boolean"
                not-null="false"
                length="1"
                />

	</class>	
</hibernate-mapping>