<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//http/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="th.go.mof.fpo.model">
	<class
		name="UserAuthentication"
		table="user_authentication"
	>
		<meta attribute="sync-DAO">false</meta>
        <id
			name="id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="userName"
			column="user_name"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="userPassword"
			column="user_password"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="thaiName"
			column="thai_name"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="englishName"
			column="english_name"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="telephone"
			column="telephone"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="email"
			column="email"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="telephoneExt"
			column="telephone_ext"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="role"
			column="role"
			type="string"
			not-null="false"
			length="3"
		/>
        <property
			name="roleDetail"
			column="role_detail"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="createDate"
			column="create_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="createBy"
			column="create_by"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="updateDate"
			column="update_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="updateBy"
			column="update_by"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="enable"
			column="enable"
			type="boolean"
			not-null="false"
			length="1"
		/>
		<property
			name="lastLogin"
			column="last_login"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="sfiRight"
			column="sfi_right"
			type="string"
			not-null="false"
			length="256"
		/>
        <property
                name="structureRight"
                column="structured_right"
                type="string"
                not-null="false"
                length="256"
                />
        <property
                name="unStructureRight"
                column="unstructured_right"
                type="string"
                not-null="false"
                length="4096"
                />
		<property
			name="supervisor"
			column="supervisor"
			type="string"
			not-null="false"
			length="256"
		/>
		<many-to-one
			name="sfi"
			column="sfi_id"
			class="Sfi"
			not-null="false"
		>
		</many-to-one>


	</class>
</hibernate-mapping>