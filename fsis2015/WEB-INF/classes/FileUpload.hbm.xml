<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.go.mof.fpo.model">
    <class
            name="FileUpload"
            table="file_upload"
            >
        <meta attribute="sync-DAO">false</meta>
        <id
                name="id"
                type="integer"
                column="id"
                >
            <generator class="identity"/>
        </id>
        <property
                name="dsDate"
                column="ds_date"
                type="timestamp"
                not-null="false"
                length="23"
                />
        <property
                name="startTime"
                column="start_time"
                type="timestamp"
                not-null="false"
                length="23"
                />
        <property
                name="endTime"
                column="end_time"
                type="timestamp"
                not-null="false"
                length="23"
                />
        <property
                name="createDate"
                column="create_date"
                type="timestamp"
                not-null="false"
                length="23"
                />
        <property
                name="createBy"
                column="create_by"
                type="string"
                not-null="false"
                length="64"
                />
        <property
                name="updateDate"
                column="update_date"
                type="timestamp"
                not-null="false"
                length="23"
                />
        <property
                name="updateBy"
                column="update_by"
                type="string"
                not-null="false"
                length="64"
                />
        <property
                name="status"
                column="status"
                type="string"
                not-null="false"
                length="3"
                />
        <property
                name="documentType"
                column="document_type"
                type="string"
                not-null="false"
                length="3"
                />
        <property
                name="description"
                column="description"
                type="string"
                not-null="false"
                length="200"
                />

        <property
                name="exportFlag"
                column="export_flag"
                type="boolean"
                not-null="false"
                length="1"
                />

        <property
                name="adjust"
                column="adjust"
                type="boolean"
                not-null="false"
                length="1"
                />
        <property
                name="adjustReason"
                column="adjust_reason"
                type="string"
                not-null="false"
                length="512"
                />
        <property
                name="adjustApprove"
                column="adjust_approve"
                type="boolean"
                not-null="false"
                length="1"
                />
       <property
                name="adjustApproveBy"
                column="adjust_approve_by"
                type="string"
                not-null="false"
                length="16"
                />
        <property
                name="adjustApproveDate"
                column="adjust_approve_date"
                type="timestamp"
                not-null="false"
                length="23"
                />
        <property
                name="adjustRejectBy"
                column="adjust_reject_by"
                type="string"
                not-null="false"
                length="16"
                />
        <property
                name="adjustRejectDate"
                column="adjust_reject_date"
                type="timestamp"
                not-null="false"
                length="23"
                />
        <property
                name="adjustRejectReason"
                column="adjust_reject_reason"
                type="string"
                not-null="false"
                length="512"
                />

        <property
                name="reportApprove"
                column="report_approve"
                type="boolean"
                not-null="false"
                length="1"
                />
       <property
                name="reportApproveBy"
                column="report_approve_by"
                type="string"
                not-null="false"
                length="16"
                />
        <property
                name="reportApproveDate"
                column="report_approve_date"
                type="timestamp"
                not-null="false"
                length="23"
                />
        <property
                name="reportRejectBy"
                column="report_reject_by"
                type="string"
                not-null="false"
                length="16"
                />
        <property
                name="reportRejectDate"
                column="report_reject_date"
                type="timestamp"
                not-null="false"
                length="23"
                />
        <property
                name="reportRejectReason"
                column="report_reject_reason"
                type="string"
                not-null="false"
                length="512"
                />

        <property
                name="enable"
                column="enable"
                type="boolean"
                not-null="false"
                length="1"
                />
        <property
                name="fileSize"
                column="file_size"
                type="string"
                not-null="false"
                length="32"
                />

        <property
                name="passComplex"
                column="pass_complex"
                type="boolean"
                not-null="false"
                length="1"
                />
        <property
                name="transfer"
                column="transfer"
                type="boolean"
                not-null="false"
                length="1"
                />
        <property
                name="cognosImportDate"
                column="cognos_import_date"
                type="timestamp"
                not-null="false"
                length="23"
                />
        <property
                name="oagRevise"
                column="oag_revise"
                type="boolean"
                />

        <set name="uploadVersions" inverse="true" cascade="delete">
            <key column="file_upload_id"/>
            <one-to-many class="UploadVersion"/>
        </set>
        
        <many-to-one
                name="sfi"
                column="sfi_id"
                class="Sfi"
                not-null="false"
                >
        </many-to-one>
        <many-to-one
                name="dsVersion"
                column="ds_version_id"
                class="DsVersion"
                not-null="false"
                >
        </many-to-one>
        <many-to-one
                name="dsTitle1"
                column="ds_title_id1"
                class="DsTitle"
                not-null="false"
                >
        </many-to-one>
        <many-to-one
                name="dsTitle2"
                column="ds_title_id2"
                class="DsTitle"
                not-null="false"
                >
        </many-to-one>
        <many-to-one
                name="dsTitle3"
                column="ds_title_id3"
                class="DsTitle"
                not-null="false"
                >
        </many-to-one>
        <many-to-one
                name="dsTitle4"
                column="ds_title_id4"
                class="DsTitle"
                not-null="false"
                >
        </many-to-one>
        <many-to-one
                name="user"
                column="user_id"
                class="UserAuthentication"
                not-null="false"
                >
        </many-to-one>

      <set name="fileUploadTransferLog" inverse="true" cascade="delete">
            <key column="file_upload_id"/>
            <one-to-many class="FileUploadTransferLog"/>
        </set>


    </class>
</hibernate-mapping>