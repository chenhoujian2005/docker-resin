<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head><title>Simple jsp page</title></head>
<body>
<table width="800px" style="border: 1px solid black;" cellpadding="2" cellspacing="0">
    <tr bgcolor="#e1e1e2">
        <td colspan="4" style="border: 1px solid black;"><strong>Priority #priority</strong></td>
    </tr>
    <tr bgcolor="#f4f4f5">
        <td width="40%" align="center" style="border: 1px solid black;">
            <strong>Title</strong>
        </td>
        <td width="20%" align="center" style="border: 1px solid black;">
            <strong>Subtitle</strong>
        </td>
        <td width="20%" align="center" style="border: 1px solid black;">
            <strong>Frequency</strong>
        </td>
        <td width="20%" align="center" style="border: 1px solid black;">
            <strong>Sending Status</strong>
        </td>
    </tr>
    <tr>
        <td style="border: 1px solid black;" align="left">#column1</td>
        <td style="border: 1px solid black;" align="center">#column2</td>
        <td style="border: 1px solid black;" align="center">#column3</td>
        <td style="border: 1px solid black;" align="center">#column4</td>
    </tr>
</table>
</body>
</html>